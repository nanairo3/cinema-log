<div class="about-main">
  <div class="container">
    <div class = "cinema-index-item">
      <% @cinemas.each do |cinema| %>
        <div class = "cinema_image">
          <%= image_tag cinema.image, :width => '33%' %>
          <p><%= cinema.name %></p>
        </div>
      <% end %>
    </div>
  </div>
</div>